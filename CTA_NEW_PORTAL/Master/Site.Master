                        $('#txtNewPassword, #txtConfirmPassword, #txtOldPassword').val("");
                        $('#txtNewPassword, #txtConfirmPassword').css({ "border": "" });
                        $("#errMsg").text('');
                    });
                //-----------------------------------------------------------------------------------------

            });
